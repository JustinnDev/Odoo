python -venv venv

pip install setuptools wheel

pip install -r requirements.txt # Para instalar los requirimentos

 python odoo-bin -r user -w password --addons-path=addons -d dbname #Inicar el sistema 

 python odoo-bin -r user -w password --addons-path=addons -d dbname -i base #Inicar el sistema borrando toda la base de datos

 python odoo-bin --without-demo=all -r user -w password --addons-path=addons -d dbname  #Para Inicar sin las demostraciones de prueba (solo cuando se esta instalando)

